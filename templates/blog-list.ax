---
layout: layout.ax
---
<h2>Blog Terbaru</h2>
<section>
  {{{ items }}}
</section>
<nav class="pagination">
  {{#if prevPage}}
    <a href="{{ prevPage }}" class="btn btn-outline">← Prev</a>
  {{/if}}
  <span>From {{ pageNum }} of {{ totalPages }}</span>
  {{#if nextPage}}
    <a href="{{ nextPage }}" class="btn btn-outline">Next →</a>
  {{/if}}
</nav>
